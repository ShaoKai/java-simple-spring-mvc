<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<layout class="ch.qos.logback.classic.PatternLayout">
			<Pattern>%d{HH:mm:ss} %-5level %logger{52} - %msg%n</Pattern>
		</layout>
	</appender>


	<logger name="com.sky.web.controller" level="debug" />
	<logger name="org.springframework.security.web.authentication.rememberme" level="debug" />

	<root level="DEBUG">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>